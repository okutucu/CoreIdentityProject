@model List<System.Security.Claims.Claim>
@{
    ViewData["Title"] = "Claims";
    Layout = "~/Views/Admin/_AdminLayout.cshtml";
}

<h4>Claims</h4>
<hr/>
<table class="table table-bordered table-stripred table-responsive">

    <tr>
        <th>Kim</th>
        <th>Dağıtıcı</th>
        <th>Ad</th>
        <th>Değer</th>
    </tr>

   
    @foreach (System.Security.Claims.Claim item in Model)
    {
        <tr>
           <td>@item.Subject.Name</td>
           <td>@item.Issuer</td>
           <td>@item.Type</td>
           <td>@item.Value</td>
        </tr>
    }
     

</table>


