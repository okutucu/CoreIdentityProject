@model UserViewModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Member/_MemberLayout.cshtml";
}

<h3 class="text-center">Kullanıcı Bilgileri</h3>
<div class="row">
   <div class="col-md-3">
       <img src="~/images/download.png" />
   </div>
   <div class="col-md-9">

       <table class="table table-bordered table-striped">
          <tr>
              <th>Kullanıcı İsmi</th>
              <td>@Model.UserName</td>

          </tr>
           <tr>
              <th>E-posta</th>
              <td>@Model.Email</td>

          </tr>
           <tr>
              <th>Telefon No</th>
              <td>@Model.PhoneNumber</td>

          </tr>



       </table>

   </div>


</div>

